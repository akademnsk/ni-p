@comment{x-kbibtex-personnameformatting=<%l><, %f>}

@article{He2019-PCM,
	abstract = "Schreibersite-(Fe, Ni)3P is a common accessory of iron meteorites, which carry direct information of planetary cores. Herein, in situ synchrotron-radiation X-ray diffraction (XRD) experiments of a natural single-crystal schreibersite have been carried out up to {\textasciitilde}{\thinspace}50 GPa in a diamond anvil cell (DAC) at room temperature. Experimental results show that schreibersite-Fe2.15Ni0.85P is very stable and remains the tetragonal structure ({\$}{\$}I{\backslash}overline {\{}4{\}}{\$}{\$}I4{\textasciimacron}) throughout the investigated pressure range. The pressure--volume (P--V) data were fitted by a third-order Birch--Murnaghan equation of state (B--M EoS), yielding K0{\thinspace}={\thinspace}184(4) GPa, V0{\thinspace}={\thinspace}365.9(1), {\$}{\$}K{\_}{\{}0{\}}^{\{}'{\}}{\$}{\$}K0{\textasciiacutex}= 4.1(2), and also a second-order B--M EoS, yielding K0{\thinspace}={\thinspace}185(1) GPa, V0{\thinspace}={\thinspace}365.8(1), {\$}{\$}K{\_}{\{}0{\}}^{\{}'{\}}{\$}{\$}K0{\textasciiacutex}= 4.0 (fixed). Unlike its isostructural minerals (e.g., Fe3S and Fe3P), schreibersite-Fe2.15Ni0.85P demonstrates an isotropic axial compressibility and an inhibition of magnetic transition under compression. Comparison in densities of Fe2.15Ni0.85P, Fe3S, Fe3P, FeS, and FeP at relative pressure--temperature conditions of the Martian core shows that the doping of Ni and P has strong effects on density reduction of iron sulfides, and schreibersite-(Fe, Ni)3P could provide a needful compensation for iron sulfides in the Martian core.",
	author = "He, Xue-Jing and Guo, Jia-Zeng and Wu, Xiang and Huang, Sheng-Xuan and Qin, Fei and Gu, Xiang-Ping and Qin, Shan",
	day = "01",
	doi = "10.1007/s00269-018-0990-x",
	issn = "1432-2021",
	journal = "Physics and Chemistry of Minerals",
	month = "Jan",
	number = "1",
	pages = "91--99",
	title = "{Compressibility of natural schreibersite up to 50 GPa}",
	url = "https://doi.org/10.1007/s00269-018-0990-x",
	volume = "46",
	year = "2019"
}

