	keywords = "VESTA 3; data visualization; electronic state calculations; Voronoi tessellation; computer programs",
	month = "Dec",
	number = "6",
	pages = "1272--1276",
	title = "{{\it VESTA3} for three-dimensional visualization of crystal, volumetric and morphology data}",
